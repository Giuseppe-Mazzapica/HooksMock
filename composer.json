{
    "name": "brain/hooks-mock",
    "description": "A mocking class utility for WordPress hooks",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/HooksMock/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/HooksMock"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "phpunit/phpunit": "~4.4"
    },
    "conflict": {
        "10up/wp_mock": "*"
    },
    "autoload": {
        "files": [
            "inc/hook-functions.php",
            "inc/assert-functions.php"
        ],
        "psr-4": {
            "Brain\\HooksMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brain\\HooksMock\\Tests\\": "tests/unit"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
